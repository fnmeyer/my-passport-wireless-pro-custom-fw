#!/bin/sh
echo "Now, CPU0 freq is \"$(cat /sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_cur_freq)\", mode:\"$(cat /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor)\""
echo "Now, CPU1 freq is \"$(cat /sys/devices/system/cpu/cpu1/cpufreq/cpuinfo_cur_freq)\", mode:\"$(cat /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor)\""

